			</if>
			<if class="CardTitle">
				<RichText background="argb(0,0,0,0)" foreground="gtc(DirectUI::ItemsView,0,0,3803)" font="messagefont;133%" margin="rect(0,0,0,4rp)"/>
			</if>
			<if class="CardSubtitle">
				<RichText background="argb(0,0,0,0)" foreground="gtc(ControlPanelStyle,8,0,3803)" font="messagefont" width="288rp" contentalign="wrapleft" margin="rect(0,0,0,12rp)"/>
			</if>
			<if class="CardWarningSubtitle">
				<RichText background="argb(0,0,0,0)" foreground="gtc(DragDrop,5,0,3803)" font="messagefont" contentalign="endellipsis" padding="rect(6rp,4rp,0,0)"/>
			</if>
			<if class="ImageHolder">
				<Element width="237rp" height="146rp" background="argb(0,0,0,0)" padding="rect(0rp,0rp,0rp,0rp)"/>
			</if>
			<if class="Image">
				<Element width="237rp" height="146rp" direction="ltr"/>
